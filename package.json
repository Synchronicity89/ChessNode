{
  "name": "chessnode",
  "version": "0.1.0",
  "private": true,
  "description": "Compact 64-bit chess engine experiments",
  "main": "src/position64.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "start": "node src/server.js"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "express": "^4.19.2",
    "@chrisoakman/chessboardjs": "1.0.0",
    "chess.js": "1.0.0",
    "jquery": "^3.7.1"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["<rootDir>/src/tests"],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
